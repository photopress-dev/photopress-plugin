		"className": {
			"type": "string"
		},
		"align": {
			"type": "string"
		},
		"postsToShow": {
			"type": "number",
			"default": 50
		},
		"displayPostContent": {
			"type": "boolean",
			"default": false
		},
		"displayPostDate": {
			"type": "boolean",
			"default": false
		},
		"displayPostLink": {
			"type": "boolean",
			"default": false
		},
		"postLink": {
			"type": "string",
